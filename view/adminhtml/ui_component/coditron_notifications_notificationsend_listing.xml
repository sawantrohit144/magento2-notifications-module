				</param>
			</childDefaults>
		</settings> -->
		<selectionsColumn name="ids">
			<settings>
				<indexField>notificationsend_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="notificationsend_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<!-- <column name="notification_id">
			<settings>
				<filter>text</filter>
				<label translate="true">Notification ID</label>
			</settings>
		</column> -->
		<actionsColumn name="actions" class="Coditron\Notifications\Ui\Component\Listing\Column\NotificationSendActions">
			<settings>
				<indexField>notificationsend_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
		<column name="customer_id">
			<settings>
				<filter>text</filter>
				<label translate="true">Customer ID</label>
			</settings>
		</column>
		<column name="message">
			<settings>
				<filter>text</filter>
				<label translate="true">Message</label>
			</settings>
		</column>
		<column name="from_date">
			<settings>
				<filter>text</filter>
				<label translate="true">From Date</label>
			</settings>
		</column>
		<column name="to_date">
			<settings>
				<filter>text</filter>
				<label translate="true">To Date</label>
			</settings>
		</column>
		<column name="status">
			<settings>
				<filter>text</filter>
				<label translate="true">Status</label>
			</settings>
		</column>
	</columns>
</listing>
